<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ahmedabadcoronabeds">

    <application
        android:allowBackup="true"
        android:icon="@drawable/alogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/alogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AhmedabadCoronaBeds">
        <activity android:name=".Action_User"></activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".ManageActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>